{
  "name": "meetup-event-planner",
  "version": "1.0.0",
  "description": "SWND P1 - Meetup Event Planner",
  "main": "index.js",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "scripts": {
    "test": "`npm bin`/mocha --compilers js:babel-core/register --require ./app/tests/test_helper.js '*/tests/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "build": "`npm bin`/webpack",
    "fakeit": "json-server app/src/data/meetup.json --routes app/src/data/routes.json",
    "dev": "`npm bin`/webpack-dev-server",
    "lint": "`npm bin`/eslint . --ext .js --ext .jsx",
    "deploy": "NODE_ENV=production `npm bin`/webpack -p",
    "start": "npm run dev",
    "clean": "rm -rf app/dist app/build",
    "setup": "npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanCCollins/react-redux-simple-starter.git"
  },
  "keywords": [
    "boilerplate",
    "redux",
    "react",
    "webpack",
    "sass",
    "css modules"
  ],
  "author": "Ryan Collins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RyanCCollins/meetups/issues"
  },
  "homepage": "https://github.com/RyanCCollins/meetups#readme",
  "dependencies": {
    "actions": "^1.3.0",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "boron": "^0.2.3",
    "css-loader": "^0.23.0",
    "fbjs": "^0.6.1",
    "file-loader": "^0.9.0",
    "foundation-sites": "^6.2.3",
    "history": "^1.14.0",
    "html-webpack-plugin": "^2.7.1",
    "immutable": "^3.7.5",
    "isomorphic-fetch": "^2.2.0",
    "leaflet": "^0.7.7",
    "lodash": "^4.13.1",
    "lru-memoize": "^1.0.1",
    "moment": "^2.14.1",
    "node-sass": "^3.4.2",
    "react": "^15.2.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-date-picker": "^5.3.16",
    "react-datepicker": "^0.28.0",
    "react-dom": "^15.0.1",
    "react-foundation": "^0.6.8",
    "react-geosuggest": "^1.24.1",
    "react-google-maps": "^4.11.0",
    "react-icons": "^2.1.0",
    "react-leaflet": "^0.12.0",
    "react-popover": "^0.4.4",
    "react-redux": "^4.4.5",
    "react-redux-toastr": "^3.1.4",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.4",
    "react-spinkit": "^1.1.7",
    "react-toastr-redux": "^0.3.0",
    "redux": "^3.5.2",
    "redux-form": "^5.2.5",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^3.2.0",
    "redux-thunk": "^1.0.0",
    "redux-toastr": "^1.1.2",
    "resolve-url-loader": "^1.4.4",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "svg-react-loader": "^0.3.5",
    "svg-url-loader": "^1.0.3",
    "webpack": "^1.12.9"
  },
